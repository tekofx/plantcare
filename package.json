{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && next dev",
    "dev:backend": "nodemon backend/src/index.ts",
    "build:frontend": "cd frontend && build",
    "analyze:frontend": "cd frontend && ANALYZE=true next build",
